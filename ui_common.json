//Item Icons
  "shield_renderer@crafting.item_renderer": {
    "property_bag": {
      "#item_id_aux": "$new_item_id_aux"
    },
    "layer": 0,
    "size": "$shield_icon_size",
    "$shield_icon_size|default": [ "100%", "100%" ],
    "$trade_center_unchanged|default": false,
    "variables": [
      {
        "requires": "($desktop_screen and $trade_center_unchanged)",
        "$shield_icon_size": [ 16, 16 ]
      }
    ],
    "bindings": [
      {
        "binding_name": "(#item_id_aux = $old_item_id_aux)",
        "binding_name_override": "#visible",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      }
    ]
  },
  "obscure_overlay_renderer": {
    "type": "image",
    "texture": "$icon_texture",
    "layer": 1,
    "size": "$obscure_icon_size",
    "$trade_center_unchanged|default": false,
    "variables": [
      {
        "requires": "($desktop_screen and $trade_center_unchanged)",
        "$obscure_icon_size": [ 16, 16 ]
      }
    ],
    "bindings": [
      {
        "binding_name": "#item_id_aux",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#item_id_aux = $item_id_aux)",
        "target_property_name": "#visible"
      }
    ]
  },
  "icon_image_renderer": {
    "type": "image",
    "texture": "$icon_texture",
    "layer": 1,
    "size": "$icon_size",
    "$trade_center_unchanged|default": false,
    "variables": [
      {
        "requires": "($desktop_screen and $trade_center_unchanged)",
        "$icon_size": [ 16, 16 ]
      }
    ],
    "bindings": [
      {
        "binding_name": "#item_id_aux",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#item_id_aux = $item_id_aux)",
        "target_property_name": "#visible"
      }
    ]
  },
  "spectrum_image_renderer": {
    "type": "image",
    "texture": "$icon_texture",
    "layer": 1,
    "size": "$spectrum_icon_size",
    "$spectrum_icon_size|default": [ "100%", "100%" ],
    "$trade_center_unchanged|default": false,
    "variables": [
      {
        "requires": "($desktop_screen and $trade_center_unchanged)",
        "$spectrum_icon_size": [ 16, 16 ]
      }
    ],
    "bindings": [
      {
        "binding_name": "#item_id_aux",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      },
      {
        "binding_type": "view",
        "source_property_name": "(not (#item_id_aux < $lower_item_id_aux) and not (#item_id_aux > $upper_item_id_aux))",
        "target_property_name": "#visible"
      }
    ]
  },
